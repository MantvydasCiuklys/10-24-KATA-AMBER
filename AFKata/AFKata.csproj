<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <RootNamespace>_</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <ContestantName>Admin</ContestantName>
    <ContestantRunTime>c#</ContestantRunTime>
    <ContestantTask>TaskA</ContestantTask>
  </PropertyGroup>

  <ItemGroup>
    <AssemblyAttribute Include="System.Reflection.AssemblyMetadataAttribute">
      <_Parameter1>ContestantName</_Parameter1>
      <_Parameter2>$(ContestantName)</_Parameter2>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.Reflection.AssemblyMetadataAttribute">
      <_Parameter1>ContestantRunTime</_Parameter1>
      <_Parameter2>$(ContestantRunTime)</_Parameter2>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.Reflection.AssemblyMetadataAttribute">
      <_Parameter1>ContestantTask</_Parameter1>
      <_Parameter2>$(ContestantTask)</_Parameter2>
    </AssemblyAttribute>
  </ItemGroup>

</Project>
